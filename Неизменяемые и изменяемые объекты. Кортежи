immutable_var = 1, 2, 3, True, 'String'
print(immutable_var)
#immutable_var[0] = 20
#print(immutable_var) #не поддерживает обращение по элементам
mutable_list = ['drink', 'milk', 'juice']
mutable_list[0] = 'coffee'
mutable_list.append(False)
mutable_list.extend(['bloat', 3])
mutable_list.remove('milk')
print(mutable_list)
